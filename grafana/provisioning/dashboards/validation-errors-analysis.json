                    "editorMode": "code",
                    "format": "table",
                    "rawQuery": true,
                    "rawSql": "SELECT \r\n  e.started_at as time,\r\n    COUNT(rl.lineage_id) FILTER (WHERE rl.validation_passed = false)::float / \r\n     NULLIF(COUNT(rl.lineage_id)::float, 0) * 100 as \"Error Rate\"\r\nFROM observability.executions e\r\nLEFT JOIN observability.record_lineage rl ON e.execution_id = rl.execution_id\r\nGROUP BY e.execution_id, e.started_at\r\nORDER BY e.started_at ASC",
