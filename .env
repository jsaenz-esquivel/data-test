# Environment configuration

# Paths (for Docker)
AIRFLOW_HOME=/usr/local/airflow
METADATA_PATH=/usr/local/airflow/dags/config/metadata.json

# Airflow config
AIRFLOW_UID=50000
AIRFLOW_GID=0
AIRFLOW__CORE__EXECUTOR=LocalExecutor
AIRFLOW__CORE__LOAD_EXAMPLES=False
AIRFLOW__CORE__DAGS_ARE_PAUSED_AT_CREATION=True

# Logging
LOG_LEVEL=INFO

# ETL paths
INPUT_PATH=/data/input/events/person
OUTPUT_PATH=/data/output/events/person
DISCARD_PATH=/data/output/discards/person